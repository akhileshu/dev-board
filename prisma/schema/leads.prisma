// # WaitlistEntry, lead-related models

model WaitlistEntry {
  id         String   @id @default(uuid())
  MVPDetailsId  String
  MVPDetails    MVPDetails  @relation(fields: [MVPDetailsId], references: [id])

  email      String
  source     String?  // e.g. "Product Hunt", "Twitter"
  joinedAt   DateTime @default(now())
}
